     ]
    }
   ],
   "source": [
    "def factorial(n):\n",
    "    result=1\n",
    "    for i in range(1,n+1):\n",
    "        result *= 1\n",
    "    return result\n",
    "print(factorial(5)) \n"
   ]
  }
